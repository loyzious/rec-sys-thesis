@Article{Isinkaye2015,
  author    = {F.O. Isinkaye and Y.O. Folajimi and B.A. Ojokoh},
  journal   = {Egyptian Informatics Journal},
  title     = {Recommendation systems: Principles, methods and evaluation},
  year      = {2015},
