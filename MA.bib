}

@InProceedings{SanchezMoreno2018,
  author    = {Diego Sanchez-Moreno and Yong Zheng and Maria N. Moreno-Garcia},
  booktitle = {2018 {IEEE}/{WIC}/{ACM} International Conference on Web Intelligence ({WI})},
